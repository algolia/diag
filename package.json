{
  "name": "diag",
  "version": "0.0.0",
  "private": "true",
  "description": "Diagnostic network issues while connecting to Algolia",
  "main": "index.js",
  "scripts": {
    "build": "webpack && ./scripts/build.js",
    "serve-build": "browser-sync start --server public/ --no-open",
    "test": "npm run lint",
    "webpack-watch": "webpack -d --watch --colors --progress",
    "browser-sync": "browser-sync start --server frontend/ --files 'frontend/*' --no-open",
    "dev": "./scripts/dev.sh",
    "lint": "eslint --quiet .",
    "deploy": "npm run build && gh-pages -d public/"
  },
  "repository": "algolia/diag",
  "keywords": [
    "algolia",
    "diagnostic"
  ],
  "dependencies": {
    "async": "^1.5.2",
    "debug": "^2.6.1",
    "entities": "1.1.1",
    "es6-promise": "^4.0.5",
    "humanize-number": "0.0.2",
    "is-mobile": "0.2.2",
    "json-loader": "^0.5.4",
    "lodash": "3.10.0",
    "prettysize": "0.0.3",
    "queue": "3.1.0",
    "superagent": "^1.8.3",
    "ua-parser-js": "^0.7.10"
  },
  "devDependencies": {
    "browser-sync": "^2.11.2",
    "bundle-collapser": "1.2.1",
    "eslint": "0.24.1",
    "gh-pages": "^0.12.0",
    "gulp": "^3.9.1",
    "gulp-csso": "^2.0.0",
    "gulp-filter": "^5.0.0",
    "gulp-if": "^2.0.2",
    "gulp-minify-html": "^1.0.6",
    "gulp-rev": "^7.1.2",
    "gulp-rev-replace": "^0.4.3",
    "gulp-uglify": "^2.0.1",
    "gulp-useref": "^3.1.2",
    "json": "^9.0.4",
    "mversion": "^1.10.1",
    "pretty-bytes": "2.0.1",
    "semver": "^5.1.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "homepage": "https://github.com/algolia/diag",
  "bugs": "https://github.com/algolia/diag/issues",
  "author": "Algolia SAS <support@algolia.com> (https://www.algolia.com)",
  "engines": {
    "node": ">=0.8"
  },
  "license": "MIT"
}
